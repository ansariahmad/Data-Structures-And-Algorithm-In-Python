     "output_type": "execute_result"
    }
   ],
   "source": [
    "nums = [2,2,1,1,1,2,2]\n",
    "majority_ele = nums[0]\n",
    "counter = 1\n",
    "\n",
    "for i in range(1, len(nums)):\n",
    "    if counter == 0:\n",
    "        majority_ele = nums[i]\n",
    "        counter = 1\n",
    "    elif majority_ele == nums[i]:\n",
    "        counter += 1\n",
    "    else:\n",
    "        counter -= 1\n",
    "majority_ele"
   ]
