   "source": [
    "## 2169 Count Operations to Obtain Zero"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def countOperations(num1: int, num2: int) -> int:\n",
    "    res = 0\n",
    "    while num2:\n",
    "        res += num1 // num2\n",
    "        num1, num2 = num2, num1%num2\n",
    "    return res"
   ]
  },
