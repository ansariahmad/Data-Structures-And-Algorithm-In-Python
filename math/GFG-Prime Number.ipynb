    "# Prime Number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "10c25ec0",
   "metadata": {},
   "outputs": [],
   "source": [
    "def isPrime(n):\n",
    "    # code here\n",
    "    counter = 0\n",
    "\n",
    "    for i in range(1, int((n**0.5)+1)):\n",
    "        if n%i == 0:\n",
    "            counter += 1\n",
    "            if n//i != i:\n",
    "                counter += 1\n",
    "    return counter == 2"
   ]
  },
