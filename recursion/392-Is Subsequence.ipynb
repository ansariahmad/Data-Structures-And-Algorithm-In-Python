    "## 392 Is Subsequence"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def isSubsequence(s: str, t: str) -> bool:\n",
    "    ptr = 0\n",
    "    for idx in range(0, len(t)):\n",
    "        if (ptr <= len(s)-1) and (t[idx] == s[ptr]):\n",
    "            ptr += 1\n",
    "    return ptr == len(s)"
   ]
  },
